        <file src="Fantomas.Cmd\bin\Release\Fantomas.dll" target="lib\Fantomas.dll" />
        <file src="Fantomas.Cmd\bin\Release\Fantomas.xml" target="lib\Fantomas.xml" />
        <file src="Fantomas.Cmd\bin\Release\FSharp.Compiler.dll" target="lib\FSharp.Compiler.dll" />
    </files>
</package>